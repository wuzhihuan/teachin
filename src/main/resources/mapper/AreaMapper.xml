<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dizhejiang.teachin.mapper.AreaMapper">

    <!--获取省份列表-->
    <select id="getProvinceList" resultType="com.dizhejiang.teachin.model.Area">
          select
                 area_id as areaId,
               area_name as areaName
         from  tb_area
         where level=0
    </select>



</mapper>