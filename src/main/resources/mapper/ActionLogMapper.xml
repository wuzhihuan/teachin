<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dizhejiang.teachin.mapper.ActionLogMapper">
<insert id="save">
    insert  into t_action_log
            (user_id,create_time,log_type,source,operate)
    values
   (#{actionLog.userId},#{actionLog.createTime},#{actionLog.logType},#{actionLog.source},#{actionLog.operate})
</insert>

</mapper>